msgstr "<abbr title=\"Abrévation\">Abrév.</abbr>"
msgid "<acronym title=\"Three-Letter Acronym\">TLA</acronym>"
msgstr "<acronym title=\"Acronyme de Trois Lettres\">ATL</acronym>"
